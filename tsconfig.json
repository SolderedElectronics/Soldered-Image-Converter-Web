{
"compilerOptions": {
"target": "ES2020",
"useDefineForClassFields": true,
"lib": ["ES2020", "DOM", "WebWorker"],
"module": "ESNext",
"skipLibCheck": true,
"jsx": "react-jsx",
"moduleResolution": "bundler",
"resolveJsonModule": true,
"isolatedModules": true,
"noEmit": true,
"strict": true,
"types": ["node"]
},
"include": ["src"]
}